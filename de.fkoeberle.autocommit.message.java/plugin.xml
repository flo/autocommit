<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="de.fkoeberle.autocommit.message.IFileContent"
            class="de.fkoeberle.autocommit.message.java.JavaFileContentFactory">
         <adapter
               type="de.fkoeberle.autocommit.message.java.JavaFileContent">
         </adapter>
      </factory>
   </extension>
   <extension
         point="de.fkoeberle.autocommit.message.factory">
      <factory
            class="de.fkoeberle.autocommit.message.java.AddedClassCMF"
            id="de.fkoeberle.autocommit.message.java.AddedClassCMF">
      </factory>
      <factory
            class="de.fkoeberle.autocommit.message.java.WorkedOnPackageCMF"
            id="de.fkoeberle.autocommit.message.java.WorkedOnPackageCMF">
      </factory>
   </extension>
   <extension
         point="de.fkoeberle.autocommit.message.profile">
      <profile
            id="de.fkoeberle.autocommit.message.profile.java.default"
            name="Default Java Profile">
         <factory
               id="de.fkoeberle.autocommit.message.java.AddedClassCMF">
         </factory>
         <factory
               id="de.fkoeberle.autocommit.message.java.WorkedOnPackageCMF"></factory>
         <factory
               id="de.fkoeberle.autocommit.message.WorkedOnPathCMF">
         </factory>
      </profile>
   </extension>

</plugin>
