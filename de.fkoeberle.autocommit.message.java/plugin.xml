<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="de.fkoeberle.autocommit.message.factory">
      <factory
            class="de.fkoeberle.autocommit.message.java.FormattedJavaFilesOfPackageCMF"
            description="Generates for commits a message in which only whitespace of java files got modified. It will tell in the commit message which java package got formatted."
            id="de.fkoeberle.autocommit.message.java.FormattedJavaFilesOfPackageCMF">
         <arguments>
            <argument
                  description="The java package which contains the changes, or when multiple java files got modified the &quot;parent&quot; package that contains the other packages. e.g. if there were changes in the package org.example.a and org.example.b then this argument will be org.example.">
            </argument>
         </arguments>
         <messages>
            <message
                  defaultValue="Formatted source in sub packages of {0}"
                  fieldName="formattedSourceInSubPackagesOfMessage">
            </message>
            <message
                  defaultValue="Formatted source in package {0}"
                  fieldName="formattedSourceInPackageMessage">
            </message>
            <message
                  defaultValue="Formatted source in the default package"
                  fieldName="formattedSourceInTheDefaultPackageMessage">
            </message>
            <message
                  defaultValue="Formatted source"
                  fieldName="formattedSourceMessage">
            </message>
         </messages>
      </factory>
      <factory
            class="de.fkoeberle.autocommit.message.java.AddedClassCMF"
            id="de.fkoeberle.autocommit.message.java.AddedClassCMF">
      </factory>
      <factory
            class="de.fkoeberle.autocommit.message.java.WorkedOnPackageCMF"
            id="de.fkoeberle.autocommit.message.java.WorkedOnPackageCMF">
      </factory>
      <factory
            class="de.fkoeberle.autocommit.message.java.WorkedOnTypeCMF"
            id="de.fkoeberle.autocommit.message.java.WorkedOnTypeCMF">
      </factory>
      <factory
            class="de.fkoeberle.autocommit.message.java.WorkedOnMethodCMF"
            id="de.fkoeberle.autocommit.message.java.WorkedOnMethodCMF">
      </factory>
      <factory
            class="de.fkoeberle.autocommit.message.java.FormattedJavaTypeCMF"
            id="de.fkoeberle.autocommit.message.java.FormattedJavaTypeCMF">
      </factory>
      <factory
            class="de.fkoeberle.autocommit.message.java.AddedMethodCMF"
            id="de.fkoeberle.autocommit.message.java.AddedMethodCMF">
      </factory>
      <factory
            class="de.fkoeberle.autocommit.message.java.DocumentedTypeCMF"
            id="de.fkoeberle.autocommit.message.java.DocumentedTypeCMF">
      </factory>
      <factory
            class="de.fkoeberle.autocommit.message.java.DocumentedMethodCMF"
            id="de.fkoeberle.autocommit.message.java.DocumentedMethodCMF">
      </factory>
      <factory
            class="de.fkoeberle.autocommit.message.java.AddedGettersAndSettersCMF"
            id="de.fkoeberle.autocommit.message.java.AddedGettersAndSettersCMF">
      </factory>
      <factory
            class="de.fkoeberle.autocommit.message.java.OrganizedImportsOfTypeCMF"
            id="de.fkoeberle.autocommit.message.java.OrganizedImportsOfTypeCMF">
      </factory>
      <factory
            class="de.fkoeberle.autocommit.message.java.DocumentedPackageCMF"
            id="de.fkoeberle.autocommit.message.java.DocumentedPackageCMF">
      </factory>
   </extension>

</plugin>
