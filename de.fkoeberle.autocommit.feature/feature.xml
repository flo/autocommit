<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="de.fkoeberle.autocommit.feature"
      label="Auto Commit Core Feature"
      version="0.0.1.alpha"
      plugin="de.fkoeberle.autocommit">

   <description>
      Allows to the user to have selected projects commit automatically to a version control system. Install also the feature &quot;Git Auto Commit Support&quot; to allow automatic commits via EGit.
   </description>

   <copyright>
      Copyright 2011-2012 Florian KÃ¶berle
   </copyright>

   <license url="http://www.eclipse.org/legal/epl-v10.html">
      This software is provided to you by the terms and conditions of the Eclipse Public License 1.0, which can be downloaded at http://www.eclipse.org/legal/epl-v10.html.
   </license>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="de.fkoeberle.autocommit" version="0.0.1" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.databinding.observable"/>
      <import plugin="de.fkoeberle.autocommit.message" version="0.0.1" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ltk.core.refactoring"/>
      <import plugin="org.eclipse.core.databinding"/>
      <import plugin="org.eclipse.core.databinding.beans"/>
      <import plugin="org.eclipse.core.databinding.property"/>
      <import plugin="org.eclipse.jface.databinding"/>
      <import plugin="com.ibm.icu"/>
      <import plugin="org.eclipse.ui.forms"/>
      <import plugin="org.eclipse.ui.ide"/>
   </requires>

   <plugin
         id="de.fkoeberle.autocommit"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="de.fkoeberle.autocommit.trigger"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="de.fkoeberle.autocommit.message"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="de.fkoeberle.autocommit.message.refactor"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="de.fkoeberle.autocommit.message.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
